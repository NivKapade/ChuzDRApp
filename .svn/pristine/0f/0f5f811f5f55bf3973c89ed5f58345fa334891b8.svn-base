<ion-content>


  <form>
    <ion-avatar item-left class="logo">
      <img padding src="assets/imgs/lock-icon.png">
    </ion-avatar>

    <h6 ion-text color="secondaryTextColor">OTP has been sent to your mobile number</h6>
    <h3 ion-text>{{mobParam}}</h3>
  
    
  <ion-item padding class="input">
      <ion-input type="tel" #otp ngControl="otp" id="otp" placeholder="Enter OTP" minlength=5 maxlength=5>
      </ion-input>
    </ion-item>


    <div text-center>
      <button ion-button round icon-end (click)="verifyOTP()">Proceed
        <ion-icon name="arrow-forward"></ion-icon>
      </button>
      <!-- <button ion-button round (click)="register()">Register</button> -->
    </div>



  </form>

  <div class="bottom">
    <img src="assets/imgs/footer.png">
  </div>


</ion-content>